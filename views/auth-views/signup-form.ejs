<a href="/"> Home </a>


<div class="signup">

  <h2> Sign Up </h2>

  <form class="form-horizontal" method="post" action="/process-signup" >

    <%  if (typeof feedbackkMessage !== 'undefined'){ %>
      <p class="error-message"> <%= feedbackkMessage %> </p>
  <%  }  %>

  <div class="form-group">

    <label for="name-input"> Name </label>
    <input id="name-input"  name="signupName" type="text">
    <br>
  </div>


  <div class="form-group">
    <label for="email-input"> Email </label>
    <input id="email-input"  name="signupEmail" type="email" placeholder="yourway@learning.com">
    <br>
  </div>

  <div class="form-group">
    <label for="password-input"> Password </label>
    <input id="password-input"  name="signupPassword" type="password">
    <br>
  </div>
    <button class="btn btn-default" > Sign Up </button>
      <a class="btn btn-default" href="/login"> Log In </a>

  </form>

</div>
